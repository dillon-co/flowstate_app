
<% if user_signed_in? %>
<div class="panel"
<div class="container">
<h1>New video</h1>

<%= form_for @video do |f| %>
  <%= render 'shared/errors', object: @video %>

  <div class="form-group">
    <%= f.label :link %>
    <%= f.text_field :link, class: 'form-control', required: true %>
    <%= f.label :title, "Add A Title, Captain (it looks better)." %>
    <%= f.text_field :title %>
    <span class="button success disabled">Youtube LInk Here.</span>
  </div>

  <%= f.submit "Flow The Video", class: 'button radius' %>
<% end %>
</div>
<% end %>
</div>
